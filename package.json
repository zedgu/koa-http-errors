{
  "name": "koa-http-errors",
  "version": "0.3.0",
  "description": "Error formatter for Koa",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --require should test/*.js --bail",
    "test-cov": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -u exports --require should test/*.js --bail",
    "test-travis": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -u exports --require should test/*.js --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zedgu/koa-http-errors.git"
  },
  "keywords": [
    "koa",
    "http",
    "status",
    "error"
  ],
  "author": "Zed Gu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zedgu/koa-http-errors/issues"
  },
  "homepage": "https://github.com/zedgu/koa-http-errors",
  "dependencies": {
    "consolidate": "^0.14.5"
  },
  "devDependencies": {
    "ejs": "*",
    "http-errors": "^1.4.0",
    "istanbul-harmony": "*",
    "koa": "^2.2.0",
    "mocha": "*",
    "should": "*",
    "supertest": "*"
  }
}
